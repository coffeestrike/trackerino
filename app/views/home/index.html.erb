<h1>Tracker v1</h1>
<p style="color: green"><%= notice %></p>

<h1>Food</h1>

<div id="food_intakes">
  <% @food_intakes.each do |food_intake| %>
    <%= render food_intake %>
  <% end %>
</div>
<hr/>

<%= link_to "New food intake", new_food_intake_path %>

<h1>Fluid</h1>

<div id="fluid_intakes">
  <% @fluid_intakes.each do |fluid_intake| %>
